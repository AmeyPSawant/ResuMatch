{
    "name": "resu-match",
    "version": "1.0.0",
    "description": "An intelligent resume scoring and matching tool.",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "start:client": "cd client && npm start",
        "start:server": "cd server && npm start",
        "build": "cd client && npm run build",
        "test": "concurrently \"npm run test:client\" \"npm run test:server\"",
        "test:client": "cd client && npm test",
        "test:server": "cd server && npm test",
        "install:all": "concurrently \"npm install\" \"cd client && npm install\" \"cd server && npm install\""
    },
    "keywords": [],
    "author": "Amey Pradip Sawant",
    "license": "MIT",
    "dependencies": {
        "concurrently": "^8.2.2"
    }
}
